{
    "name": "gouter/valueobjects",
    "type": "library",
    "description": "バリューオブジェクトパック",
    "keywords": [
        "private",
        "package",
        "ddd",
        "valueobject"
    ],
    "license": "Gouter Co.,Ltd. all rights reserved.",
    "require": {
        "php": "^7.3|^8.0",
        "ramsey/collection": "^1.1",
        "ramsey/uuid": "^4.1",
        "nesbot/carbon": "^2.53"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.1",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Gouter\\ValueObjects\\": "src/ValueObjects/"
        },
        "classmap": [

        ],
        "files": [

        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Gouter\\ValueObjects\\Tests\\": "src/tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "sniffer": [
            "./vendor/bin/phpcs --standard=./phpcs.xml ./"
        ],
        "sniffer-bf": [
            "./vendor/bin/phpcbf --standard=./phpcs.xml ./"
        ],
        "sniffer-report-csv": [
            "./endor/bin/phpcs --standard=./phpcs.xml --report=csv --report-file=./storage/logs/sniffer-report.csv"
        ],
        "fix-diff": [
            "./vendor/bin/php-cs-fixer fix --dry-run --diff"
        ],
        "fix": [
            "./vendor/bin/php-cs-fixer fix"
        ]
    }
}
